  },
  "scripts": {
    "dev": "babel-node scripts/development.js",
    "build": "better-npm-run clean && better-npm-run build:src && better-npm-run build:config && better-npm-run build:lang && better-npm-run build:prod",
    "prod": "node dist/production.js",
    "clean": "better-npm-run clean"
  },
  "betterScripts": {
    "clean": "del './dist'",
    "build:config": "babel src/config.js -o dist/config.js",
    "build:lang": "babel src/i18n.js -o dist/i18n.js",
    "build:src": {
      "command": "webpack --config webpack/webpack.production.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:prod": "babel scripts/production.js -o dist/production.js"
  }
}
